{
  "listener":  {
    "tcp":  {
      "address": "0.0.0.0:8200",
      "tls_cert_file": "/vault/config/vault.crt",
      "tls_key_file": "/vault/config/vault.key"
    }
  },
  "backend": {
    "file": {
      "path": "/vault/file"
    }
  },
  "default_lease_ttl": "168h",
  "max_lease_ttl": "0h",
  "api_addr": "http://0.0.0.0:8200",
  "ui": true
}